{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## 📚 Documentation",
      "labels": ["doc"]
    },
    {
      "title": "## 🛠 Under the hood",
      "labels": []
    }
  ],
  "template": "${{CHANGELOG}}",
  "pr_template": "- [${{TITLE}}](https://github.com/smartqasa/sonos-player/commit/${{MERGE_SHA}})",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.): (.+)",
      "target": "$1"
    }
  ]
}
